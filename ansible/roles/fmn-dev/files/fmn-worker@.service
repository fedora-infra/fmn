[Unit]
Description=Celery worker to find FMN message recipients
After=network.target
Documentation=https://github.com/fedora-infra/fmn/

[Service]
Type=simple
ExecStart=/home/vagrant/.virtualenvs/fmn/bin/celery -A fmn worker --pool=prefork --concurrency=1 -E -l info -n fmn-worker%i@%H

[Install]
WantedBy=multi-user.target

[Install]
WantedBy=multi-user.target
